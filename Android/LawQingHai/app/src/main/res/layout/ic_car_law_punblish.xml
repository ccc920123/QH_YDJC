<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="wrap_content">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        android:padding="10dp">

        <ImageView
            android:layout_width="10dp"
            android:layout_height="10dp"
            android:src="@drawable/circle_qing"/>
        <TextView
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:text="@string/wgxwcf"
            android:textSize="20sp"
            android:layout_marginLeft="5dp"
            android:textColor="#5aaae7"/>

    </LinearLayout>

        <TableLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5.0sp"
            android:layout_marginRight="5.0sp"
            android:stretchColumns="1" >

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="#ff000000" >

                <TextView
                    style="@style/table_text01"
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginLeft="1.0dip"
                    android:layout_marginTop="1.0dip"
                    android:visibility="gone"
                    android:text="@string/cfzl"/>

                <TextView
                    android:id="@+id/cfzl"
                    android:layout_weight="1.0"
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginLeft="1.0dip"
                    android:layout_marginTop="1.0dip"
                    android:visibility="gone"
                    style="@style/table_text02"/>

                <TextView
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginLeft="1.0dip"
                    android:layout_marginTop="1.0dip"
                    android:text="@string/wfdm01"
                    style="@style/table_text01" />

                <TextView
                    android:id="@+id/wfdm"
                    android:layout_weight="3.0"
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginLeft="1.0dip"
                    android:layout_marginRight="1.0dip"
                    android:layout_marginTop="1.0dip"
                    style="@style/table_text02" />
            </TableRow>

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="#ff000000" >

                <TextView
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginLeft="1.0dip"
                    android:text="@string/wfjf"
                    style="@style/table_text01"/>

                <TextView
                    android:id="@+id/wfjf"
                    android:layout_weight="1.0"
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginLeft="1.0dip"
                    style="@style/table_text02" />

                <TextView
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginLeft="1.0dip"
                    android:text="@string/fkje"
                    style="@style/table_text01"/>

                <TextView
                    android:id="@+id/fkje"
                    android:layout_weight="1.0"
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginLeft="1.0dip"
                    android:layout_marginRight="1.0dip"
                    style="@style/table_text02" />
            </TableRow>

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:visibility="gone"
                android:background="#ff000000" >

                <TextView
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginLeft="1.0dip"
                    android:text="@string/zkys"
                    style="@style/table_text01"/>

                <TextView
                    android:id="@+id/zkys"
                    android:layout_weight="1.0"
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginLeft="1.0dip"
                    style="@style/table_text02" />

                <TextView
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginLeft="1.0dip"
                    android:text="@string/jlts"
                    style="@style/table_text01"/>

                <TextView
                    android:id="@+id/jlts"
                    android:layout_weight="1.0"
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginLeft="1.0dip"
                    android:layout_marginRight="1.0dip"
                    style="@style/table_text02" />
            </TableRow>

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="#ff000000" >

                <TextView
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginLeft="1.0dip"
                    android:text="@string/jbr"
                    style="@style/table_text01" />

                <TextView
                    android:id="@+id/jbr"
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginLeft="1.0dip"
                    style="@style/table_text02"
                    android:layout_weight="1.0"/>
                
                <LinearLayout
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginLeft="1.0dip"
                    style="@style/table_text01">

                    <TextView
                        android:id="@+id/jkbj_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="14sp"
                        android:textColor="@color/black_text01"
                        android:text="@string/jkbj"/>
                </LinearLayout>

                <TextView
                    android:id="@+id/jkbj"
                    android:layout_weight="1.0"
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginLeft="1.0dip"
                    android:layout_marginRight="1.0dip"
                    style="@style/table_text02" />
            </TableRow>
            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="#ff000000" >

                <TextView
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginLeft="1.0dip"
                    android:text="@string/cflb"
                    style="@style/table_text01" />

                <TextView
                    android:id="@+id/cflb"
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginLeft="1.0dip"
                    android:layout_marginRight="1dp"
                    style="@style/table_text02"
                    android:layout_weight="3.0"/>

            </TableRow>

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="#ff000000" >

                <TextView
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginLeft="1.0dip"
                    android:text="@string/cfbh"
                    style="@style/table_text01" />

                <TextView
                    android:id="@+id/cfbh"
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginLeft="1.0dip"
                    android:layout_marginRight="1dp"
                    style="@style/table_text02"
                    android:layout_weight="3.0"/>

            </TableRow>
            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="#ff000000" >

                <TextView
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginLeft="1.0dip"
                    android:text="@string/clsj"
                    style="@style/table_text01" />

                <TextView
                    android:id="@+id/clsj"
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginLeft="1.0dip"
                    android:layout_marginRight="1dp"
                    style="@style/table_text02"
                    android:layout_weight="3.0"/>

            </TableRow>

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="#ff000000" >

                <TextView
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginLeft="1.0dip"
                    android:text="@string/cjjg"
                    style="@style/table_text01" />

                <TextView
                    android:id="@+id/cjjg"
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginLeft="1.0dip"
                    android:layout_marginRight="1dp"
                    style="@style/table_text02"
                    android:layout_weight="3.0"/>

            </TableRow>
        </TableLayout>

</LinearLayout>